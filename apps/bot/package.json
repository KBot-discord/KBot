{
	"name": "@kbotdev/bot",
	"version": "2.0.0",
	"description": "A multi-feature Discord bot",
	"author": "@kbotdev",
	"license": "AGPL-3.0-or-later",
	"main": "./dist/KBot.js",
	"private": true,
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/KBot-discord/kbot.git",
		"directory": "apps/bot"
	},
	"homepage": "https://github.com/KBot-discord/kbot/tree/main/apps/bot",
	"engines": {
		"node": ">=18.x"
	},
	"scripts": {
		"build": "yarn db:generate && tsup",
		"dev": "tsup --watch --onSuccess \"yarn start\"",
		"start": "node --enable-source-maps ./dist/KBot.js",
		"start:migrate": "prisma migrate deploy && yarn start",
		"lint": "eslint --fix --ext ts src tests",
		"format": "prettier --write --loglevel=warn \"{src,tests}/**/*.{js,ts,json}\"",
		"test": "vitest",
		"db:generate": "prisma generate",
		"db:push": "prisma db push"
	},
	"dependencies": {
		"@bufbuild/connect-node": "^0.8.6",
		"@bufbuild/connect-web": "^0.8.6",
		"@bufbuild/protobuf": "^1.2.0",
		"@kbotdev/holodex": "workspace:^",
		"@kbotdev/menus": "^1.0.3",
		"@kbotdev/plugin-modules": "^1.0.7",
		"@kbotdev/proto": "*",
		"@prisma/client": "^4.13.0",
		"@sapphire/decorators": "^6.0.1",
		"@sapphire/discord.js-utilities": "^6.0.6",
		"@sapphire/duration": "^1.1.0",
		"@sapphire/fetch": "^2.4.1",
		"@sapphire/framework": "^4.4.1",
		"@sapphire/plugin-api": "^5.0.3",
		"@sapphire/plugin-logger": "^3.0.3",
		"@sapphire/plugin-scheduled-tasks": "^6.0.1",
		"@sapphire/utilities": "^3.11.1",
		"@sentry/integrations": "^7.49.0",
		"@sentry/node": "^7.49.0",
		"@skyra/gifenc": "^1.0.1",
		"bullmq": "^3.12.0",
		"canvas": "2.11.2",
		"canvas-constructor": "^7.0.0",
		"discord-api-types": "^0.37.39",
		"discord.js": "^14.9.0",
		"dotenv": "^16.0.3",
		"fuse.js": "^6.6.2",
		"humanize-duration": "^3.28.0",
		"ioredis": "^5.3.2",
		"meilisearch": "^0.32.3",
		"prom-client": "^14.2.0"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^4.4.1",
		"@sapphire/prettier-config": "^1.4.5",
		"@sentry/types": "^7.49.0",
		"@swc/core": "^1.3.53",
		"@types/humanize-duration": "^3.27.1",
		"@types/node": "^18.15.13",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"@vitest/coverage-c8": "^0.30.1",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.1",
		"prettier": "^2.8.7",
		"prisma": "^4.13.0",
		"tsc-watch": "^6.0.0",
		"tsup": "^6.7.0",
		"typescript": "^5.0.4",
		"vitest": "^0.30.1"
	},
	"prettier": "@sapphire/prettier-config",
	"lint-staged": {
		"*.{js,ts,json}": [
			"eslint --fix",
			"prettier --write --loglevel=warn \"{src,tests}/**/*.{js,ts,json}\""
		]
	},
	"imports": {
		"#prisma": "./prisma/generated/index.js",
		"#config": "./dist/config.js",
		"#hooks/*": "./dist/lib/hooks/*.js",
		"#extensions/*": "./dist/lib/extensions/*.js",
		"#observability/*": "./dist/lib/observability/*.js",
		"#services": "./dist/lib/services/index.js",
		"#services/*": "./dist/lib/services/*.js",
		"#structures/*": "./dist/lib/structures/*.js",
		"#types/*": "./dist/lib/types/*.js",
		"#utils/*": "./dist/lib/utilities/*.js",
		"#utils/customIds": "./dist/lib/utilities/customIds/index.js",
		"#utils/validators": "./dist/lib/utilities/validators/index.js",
		"#modules/*": "./dist/modules/*.js",
		"#rpc/middlewares": "./dist/rpc/middlewares/index.js",
		"#rpc/middlewares/*": "./dist/rpc/middlewares/*.js",
		"#rpc/services": "./dist/rpc/services/index.js",
		"#rpc/services/*": "./dist/rpc/services/*.js",
		"#rpc/server": "./dist/rpc/server.js"
	}
}
