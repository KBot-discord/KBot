{
	"name": "@kbotdev/bot",
	"version": "2.0.0",
	"description": "A multi-feature Discord bot",
	"author": "KBot",
	"license": "AGPL-3.0-or-later",
	"main": "./dist/index.js",
	"engines": {
		"node": "^18.x"
	},
	"scripts": {
		"build": "tsc -b src",
		"dev": "tsc-watch --noClear -p ./src/tsconfig.json --onSuccess \"yarn run start\"",
		"start": "node ./dist/index.js",
		"prepare": "yarn buf:generate && yarn build",
		"lint": "eslint --fix --ext .ts src tests prisma",
		"format": "prettier --write --loglevel=warn \"{src,tests,prisma}/**/*.{js,ts,json}\"",
		"test": "vitest",
		"ci:build": "yarn db:generate && yarn build",
		"ci:test": "vitest run --coverage",
		"db:generate": "ts-node ./prisma && prisma generate",
		"db:dev": "prisma migrate dev",
		"db:push": "prisma db push",
		"db:deploy": "prisma migrate deploy",
		"buf:generate": "rm -r -f src/rpc/gen && yarn buf:bot && yarn buf:youtube",
		"buf:bot": "buf generate buf.build/kbotdev/bot",
		"buf:youtube": "buf generate buf.build/kbotdev/youtube"
	},
	"dependencies": {
		"@discordjs/builders": "^1.3.0",
		"@grpc/grpc-js": "^1.8.0",
		"@kbotdev/plugin-modules": "^0.1.2",
		"@prisma/client": "^4.7.1",
		"@sapphire/decorators": "^5.0.0",
		"@sapphire/discord.js-utilities": "^5.1.2",
		"@sapphire/duration": "^1.0.0",
		"@sapphire/framework": "^3.1.4",
		"@sapphire/plugin-api": "^4.0.1",
		"@sapphire/plugin-logger": "^3.0.1",
		"@sapphire/plugin-scheduled-tasks": "^6.0.0",
		"@sapphire/plugin-subcommands": "^3.2.4",
		"@sapphire/utilities": "^3.11.0",
		"@sentry/integrations": "^7.24.2",
		"@sentry/node": "^7.24.2",
		"@skyra/gifenc": "^1.0.1",
		"axios": "^1.2.1",
		"binarytf": "^2.1.2",
		"bullmq": "^3.4.1",
		"canvas": "^2.10.2",
		"canvas-constructor": "^7.0.0",
		"discord-api-types": "^0.33.5",
		"discord.js": "^13.12.0",
		"express": "^4.18.2",
		"google-protobuf": "^3.21.2",
		"ioredis": "^5.2.4",
		"prom-client": "^14.1.0",
		"schemix": "^1.8.4"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/prettier-config": "^1.4.4",
		"@sapphire/ts-config": "^3.3.4",
		"@sentry/types": "^7.24.2",
		"@types/express": "^4.17.14",
		"@types/node": "^18.11.13",
		"@typescript-eslint/eslint-plugin": "^5.46.0",
		"@typescript-eslint/parser": "^5.46.0",
		"@vitest/coverage-istanbul": "^0.25.7",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"grpc-tools": "^1.12.3",
		"grpc_tools_node_protoc_ts": "^5.3.2",
		"husky": "^8.0.2",
		"lint-staged": "^13.1.0",
		"prettier": "^2.8.1",
		"prisma": "^4.7.1",
		"ts-node": "^10.9.1",
		"tsc-watch": "^6.0.0",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vitest": "^0.25.7"
	},
	"prettier": "@sapphire/prettier-config",
	"lint-staged": {
		"*.{mjs,ts,js}": [
			"eslint --fix",
			"prettier --write --loglevel=warn \"{src,tests,prisma}/**/*.{js,ts,json}\""
		]
	},
	"packageManager": "yarn@3.3.0"
}
