{
	"name": "@kbotdev/prisma",
	"version": "0.0.0",
	"license": "AGPL-3.0-or-later",
	"private": true,
	"main": "src/index.ts",
	"types": "src/index.ts",
	"scripts": {
		"lint": "eslint --ext ts src",
		"lint:fix": "eslint --fix --ext ts src",
		"format": "prettier --write --log-level=warn \"prisma/**/*.{js,ts,json}\" && prisma format",
		"schemix": "ts-node ./prisma/schemix.ts",
		"db:generate": "yarn schemix && prisma generate",
		"db:push": "prisma db push",
		"db:sync": "yarn db:generate && yarn db:push",
		"db:deploy": "prisma migrate deploy",
		"db:migrate": "prisma migrate reset && prisma migrate dev"
	},
	"dependencies": {
		"@prisma/client": "^5.5.2",
		"@types/node": "^20.8.10"
	},
	"devDependencies": {
		"eslint": "8.52.0",
		"prettier": "^3.0.3",
		"prisma": "^5.5.2",
		"schemix": "^1.14.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	}
}
