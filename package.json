{
	"name": "@kbotdev/bot",
	"version": "2.0.0",
	"description": "A multi-feature Discord bot",
	"author": "@kbotdev",
	"license": "AGPL-3.0-or-later",
	"main": "./dist/index.js",
	"engines": {
		"node": "^18.x"
	},
	"scripts": {
		"prepare": "yarn buf:generate && yarn db:generate",
		"build": "tsc -b src",
		"dev": "tsc-watch --noClear -p src/tsconfig.json --onSuccess \"yarn run start\"",
		"start": "node ./dist/index.js",
		"start:migrate": "yarn db:deploy && node ./dist/index.js",
		"lint": "eslint --fix --ext ts src tests prisma",
		"format": "prettier --write --loglevel=warn \"{src,tests,prisma}/**/*.{js,ts,json}\"",
		"test": "vitest",
		"ci:build": "yarn db:generate && yarn build",
		"ci:test": "vitest run --coverage",
		"db:generate": "ts-node ./prisma && prisma generate",
		"db:dev": "prisma migrate dev",
		"db:push": "prisma db push",
		"db:deploy": "prisma migrate deploy",
		"buf:generate": "rm -r -f src/rpc/gen && yarn buf:bot && yarn buf:types",
		"buf:bot": "buf generate buf.build/kbotdev/bot",
		"buf:types": "buf generate buf.build/kbotdev/types"
	},
	"dependencies": {
		"@bufbuild/connect-node": "^0.8.3",
		"@bufbuild/connect-web": "^0.8.3",
		"@bufbuild/protobuf": "^1.1.0",
		"@googleapis/youtube": "^8.0.2",
		"@kbotdev/menus": "^1.0.2",
		"@kbotdev/plugin-modules": "^1.0.6",
		"@prisma/client": "^4.11.0",
		"@sapphire/decorators": "^6.0.0",
		"@sapphire/discord.js-utilities": "^6.0.2",
		"@sapphire/duration": "^1.0.0",
		"@sapphire/fetch": "^2.4.1",
		"@sapphire/framework": "^4.2.0",
		"@sapphire/plugin-api": "^5.0.1",
		"@sapphire/plugin-i18next": "^5.0.1",
		"@sapphire/plugin-logger": "^3.0.1",
		"@sapphire/plugin-scheduled-tasks": "^6.0.0",
		"@sapphire/utilities": "^3.11.0",
		"@sentry/integrations": "^7.41.0",
		"@sentry/node": "^7.41.0",
		"@skyra/gifenc": "^1.0.1",
		"binarytf": "^2.1.2",
		"bullmq": "^3.10.1",
		"canvas": "2.10.2",
		"canvas-constructor": "^7.0.0",
		"dayjs": "^1.11.7",
		"discord-api-types": "^0.37.35",
		"discord.js": "^14.7.1",
		"dotenv": "^16.0.3",
		"fast-xml-parser": "^4.1.3",
		"humanize-duration": "^3.28.0",
		"ioredis": "^5.3.1",
		"meilisearch": "^0.31.1",
		"prom-client": "^14.2.0"
	},
	"devDependencies": {
		"@bufbuild/protoc-gen-connect-es": "^0.8.3",
		"@bufbuild/protoc-gen-es": "^1.1.0",
		"@microsoft/tsdoc": "^0.14.2",
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^3.3.4",
		"@sentry/types": "^7.41.0",
		"@types/humanize-duration": "^3.27.1",
		"@types/node": "^18.14.6",
		"@typescript-eslint/eslint-plugin": "^5.54.1",
		"@typescript-eslint/parser": "^5.54.1",
		"@vitest/coverage-c8": "^0.29.2",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-import-resolver-typescript": "^3.5.3",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.2",
		"prettier": "^2.8.4",
		"prisma": "^4.11.0",
		"schemix": "^1.9.1",
		"ts-node": "^10.9.1",
		"tsc-watch": "^6.0.0",
		"typescript": "^4.9.5",
		"vitest": "^0.29.2"
	},
	"resolutions": {
		"discord-api-types": "0.37.20"
	},
	"prettier": "@sapphire/prettier-config",
	"lint-staged": {
		"*.{mjs,ts,js}": [
			"eslint --fix",
			"prettier --write --loglevel=warn \"{src,tests,prisma}/**/*.{js,ts,json}\""
		]
	},
	"packageManager": "yarn@3.4.1",
	"imports": {
		"#prisma": "./prisma/generated/index.js",
		"#hooks/register": "./dist/lib/hooks/register.js",
		"#extensions/*": "./dist/lib/extensions/*.js",
		"#observability/*": "./dist/lib/observability/*.js",
		"#services/core": "./dist/lib/services/core/index.js",
		"#services/events": "./dist/lib/services/events/index.js",
		"#services/moderation": "./dist/lib/services/moderation/index.js",
		"#services/premium": "./dist/lib/services/premium/index.js",
		"#services/twitch": "./dist/lib/services/twitch/index.js",
		"#services/utility": "./dist/lib/services/utility/index.js",
		"#services/welcome": "./dist/lib/services/welcome/index.js",
		"#services/youtube": "./dist/lib/services/youtube/index.js",
		"#structures/*": "./dist/lib/structures/*.js",
		"#types/*": "./dist/lib/types/*.js",
		"#types/premium/*": "./dist/lib/types/premium/index.js",
		"#utils/*": "./dist/lib/utilities/*.js",
		"#utils/customIds": "./dist/lib/utilities/customIds/index.js",
		"#utils/validators": "./dist/lib/utilities/validators/index.js",
		"#modules/*": "./dist/modules/*.js",
		"#rpc/middlewares": "./dist/rpc/middlewares/index.js",
		"#rpc/bot": "./dist/rpc/bot.js",
		"#rpc/server": "./dist/rpc/server.js",
		"#rpc/types": "./dist/rpc/types.js",
		"#rpc/utils": "./dist/rpc/utils.js"
	}
}
