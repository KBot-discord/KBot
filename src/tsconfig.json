{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"outDir": "../dist",
		"rootDir": ".",
		"baseUrl": ".",
		"noEmit": true,
		"typeRoots": ["./lib/types/**/*", "../node_modules/@types"],
		"paths": {
			"#utils/*": ["lib/util/*"],
			"#lib/*": ["lib/*"],
			"#services/*": ["lib/services/*"]
		},
		"allowJs": true,
		"noImplicitAny": true
	},
	"include": ["."],
	"exclude": ["./tsconfig.json"]
}
