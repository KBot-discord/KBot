{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"outDir": "./dist",
		"rootDir": "./src",
		"baseUrl": "./src",
		"tsBuildInfoFile": "./dist/.tsbuildinfo",
		"noEmit": true,
		"incremental": true,
		"typeRoots": ["./src/lib/types/**/*", "../../node_modules/@types"],
		"paths": {
			"#config": ["config.ts"],
			"#extensions/*": ["lib/extensions/*"],
			"#observability/*": ["lib/observability/*"],
			"#repositories/*": ["lib/repositories/*"],
			"#services/*": ["lib/services/*"],
			"#services": ["lib/services/index.ts"],
			"#structures/*": ["lib/structures/*"],
			"#types/*": ["lib/types/*"],
			"#utils/*": ["lib/utilities/*"],
			"#modules/*": ["modules/*"],
			"#plugins/*": ["plugins/*"]
		}
	},
	"include": ["src"]
}
