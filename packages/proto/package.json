{
	"name": "@kbotdev/proto",
	"version": "0.0.0",
	"license": "AGPL-3.0-or-later",
	"private": true,
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"types": "./dist.index.d.ts"
		}
	},
	"scripts": {
		"build": "yarn buf:generate && tsup",
		"buf:generate": "rm -r -f src/gen && yarn buf:bot && yarn buf:types",
		"buf:bot": "buf generate ../../protos/bot",
		"buf:types": "buf generate ../../protos/types",
		"lint": "eslint --fix --ext ts src",
		"format": "prettier --write --loglevel=warn \"src/**/*.ts\""
	},
	"devDependencies": {
		"@bufbuild/protoc-gen-connect-es": "^0.8.6",
		"@bufbuild/protoc-gen-es": "^1.2.0",
		"@sapphire/ts-config": "^4.0.0",
		"tsup": "^6.7.0"
	}
}
