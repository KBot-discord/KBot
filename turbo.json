{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"dist/**",
				"build/**"
			],
			"cache": true
		},
		"lint": {
			"dependsOn": [
				"^build"
			],
			"outputs": [],
			"cache": true
		},
		"test": {
			"outputs": [],
			"cache": true
		},
		"dev": {
			"cache": false
		},
		"db:generate": {
			"outputs": [
				"apps/bot/prisma/schema.prisma",
				"apps/bot/prisma/generated"
			],
			"cache": false
		},
		"db:push": {
			"inputs": [
				"apps/bot/prisma/schema.prisma"
			],
			"cache": false
		},
		"db:deploy": {
			"inputs": [
				"apps/bot/prisma/schema.prisma"
			],
			"cache": false
		}
	}
}
