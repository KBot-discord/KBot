{
	"name": "@kbotdev/bot",
	"version": "2.0.0",
	"description": "A multi-feature Discord bot",
	"author": "@kbotdev",
	"license": "AGPL-3.0-or-later",
	"type": "module",
	"main": "./dist/KBot.js",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/KBot-discord/kbot.git",
		"directory": "apps/bot"
	},
	"homepage": "https://github.com/KBot-discord/kbot/tree/main/apps/bot",
	"engines": {
		"node": ">=20.x"
	},
	"scripts": {
		"build": "tsup",
		"check": "tsc",
		"dev": "tsup --watch --onSuccess \"yarn start\"",
		"start": "node --preserve-symlinks --enable-source-maps ./dist/KBot.js",
		"lint": "eslint --ext ts src tests && yarn check",
		"lint:fix": "eslint --fix --ext ts src tests",
		"format": "prettier --write --loglevel=warn \"{src,tests}/**/*.{js,ts,json}\"",
		"test": "vitest"
	},
	"dependencies": {
		"@bufbuild/connect": "^0.9.0",
		"@bufbuild/connect-node": "^0.9.0",
		"@bufbuild/connect-web": "^0.9.0",
		"@bufbuild/protobuf": "^1.2.0",
		"@kbotdev/database": "workspace:^",
		"@kbotdev/holodex": "workspace:^",
		"@kbotdev/meili": "workspace:^",
		"@kbotdev/plugin-modules": "^2.0.0",
		"@kbotdev/proto": "workspace:^",
		"@kbotdev/redis": "workspace:^",
		"@sapphire/decorators": "^6.0.1",
		"@sapphire/discord.js-utilities": "^7.0.0",
		"@sapphire/duration": "^1.1.0",
		"@sapphire/fetch": "^2.4.1",
		"@sapphire/framework": "^4.4.4",
		"@sapphire/plugin-api": "^5.0.4",
		"@sapphire/plugin-scheduled-tasks": "^7.1.0",
		"@sentry/integrations": "^7.53.1",
		"@sentry/node": "^7.53.1",
		"@skyra/gifenc": "^1.0.1",
		"bullmq": "^3.14.1",
		"canvas": "2.11.2",
		"canvas-constructor": "^7.0.1",
		"colorette": "^2.0.20",
		"discord.js": "^14.11.0",
		"dotenv": "^16.0.3",
		"fuzzysort": "^2.0.4",
		"humanize-duration": "^3.28.0",
		"prom-client": "^14.2.0"
	},
	"devDependencies": {
		"@kbotdev/eslint-config": "workspace:^",
		"@sentry/types": "^7.53.1",
		"@swc/core": "^1.3.60",
		"@types/humanize-duration": "^3.27.1",
		"eslint": "^8.41.0",
		"prettier": "^2.8.8",
		"tsup": "^6.7.0",
		"typescript": "^5.0.4",
		"vitest": "^0.31.1"
	},
	"imports": {
		"#prisma": "./prisma/generated/index.js",
		"#config": "./dist/config.js",
		"#extensions/*": "./dist/lib/extensions/*.js",
		"#observability/*": "./dist/lib/observability/*.js",
		"#grpc/*": "./dist/lib/grpc/*.js",
		"#grpc/errors": "./dist/lib/grpc/errors/index.js",
		"#grpc/middlewares": "./dist/lib/grpc/middlewares/index.js",
		"#grpc/utils": "./dist/lib/grpc/utils.js",
		"#services": "./dist/lib/services/index.js",
		"#services/*": "./dist/lib/services/*.js",
		"#structures/*": "./dist/lib/structures/*.js",
		"#structures/errors": "./dist/lib/structures/errors/index.js",
		"#types/*": "./dist/lib/types/*.js",
		"#utils/*": "./dist/lib/utilities/*.js",
		"#utils/validators": "./dist/lib/utilities/validators/index.js",
		"#modules/*": "./dist/modules/*.js",
		"#plugins/*": "./dist/plugins/*.js",
		"#plugins/grpc": "./dist/plugins/grpc/index.js"
	}
}
