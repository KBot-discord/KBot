{
	"extends": "../../../tsconfig.base.json",
	"compilerOptions": {
		"outDir": "../dist",
		"rootDir": ".",
		"baseUrl": ".",
		"skipLibCheck": true,
		"tsBuildInfoFile": "../dist/.tsbuildinfo",
		"typeRoots": ["./lib/types/**/*", "../../../node_modules/@types"],
		"paths": {
			"#config": ["config.ts"],
			"#extensions/*": ["lib/extensions/*"],
			"#hooks/*": ["lib/hooks/*"],
			"#observability/*": ["lib/observability/*"],
			"#services/*": ["lib/services/*"],
			"#services": ["lib/services/index.ts"],
			"#structures/*": ["lib/structures/*"],
			"#types/*": ["lib/types/*"],
			"#types/CustomIds": ["lib/types/CustomIds/index.ts"],
			"#types/database": ["lib/types/database/index.ts"],
			"#utils/*": ["lib/utilities/*"],
			"#modules/*": ["modules/*"],
			"#rpc/*": ["rpc/*"]
		}
	},
	"include": ["."]
}
