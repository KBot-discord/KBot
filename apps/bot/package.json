{
	"name": "@kbotdev/bot",
	"version": "3.0.0",
	"description": "A multi-feature Discord bot",
	"author": "@kbotdev",
	"license": "AGPL-3.0-or-later",
	"type": "module",
	"main": "./dist/KBot.js",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/KBot-discord/kbot.git",
		"directory": "apps/bot"
	},
	"homepage": "https://github.com/KBot-discord/kbot/tree/main/apps/bot",
	"engines": {
		"node": ">=20.x"
	},
	"scripts": {
		"build": "tsup",
		"check": "tsc",
		"dev": "tsup --watch --onSuccess \"yarn start\"",
		"start": "node --preserve-symlinks --enable-source-maps ./dist/KBot.js",
		"lint": "eslint --ext ts src tests && yarn check",
		"lint:fix": "eslint --fix --ext ts src tests",
		"format": "prettier --write --log-level=warn \"{src,tests}/**/*.{js,ts,json}\" && prisma format",
		"test": "vitest --watch=false",
		"test:watch": "vitest -w",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:sync": "yarn db:generate && yarn db:push",
		"db:deploy": "prisma migrate deploy",
		"db:migrate": "prisma migrate reset && prisma migrate dev"
	},
	"dependencies": {
		"@kbotdev/holodex": "workspace:^",
		"@kbotdev/meili": "workspace:^",
		"@kbotdev/plugin-modules": "^3.0.0",
		"@killbasa/redis-utils": "^0.0.5",
		"@sapphire/decorators": "^6.0.3",
		"@sapphire/discord.js-utilities": "^7.1.3",
		"@sapphire/duration": "^1.1.1",
		"@sapphire/fetch": "^3.0.0",
		"@sapphire/framework": "^5.0.4",
		"@sapphire/plugin-api": "^6.0.1",
		"@sapphire/plugin-logger": "^4.0.1",
		"@sapphire/plugin-scheduled-tasks": "^9.0.1",
		"@sapphire/utilities": "^3.14.0",
		"@sentry/integrations": "^7.86.0",
		"@sentry/node": "^7.86.0",
		"@skyra/gifenc": "^1.0.1",
		"bufferutil": "^4.0.8",
		"bullmq": "^4.15.2",
		"canvas": "2.11.2",
		"canvas-constructor": "^7.0.1",
		"colorette": "^2.0.20",
		"discord.js": "^14.14.1",
		"dotenv": "^16.3.1",
		"fuzzysort": "^2.0.4",
		"humanize-duration": "^3.31.0",
		"prom-client": "^15.0.0",
		"utf-8-validate": "^6.0.3",
		"zlib-sync": "^0.1.9"
	},
	"devDependencies": {
		"@prisma/client": "^5.7.0",
		"@sentry/types": "^7.86.0",
		"@types/humanize-duration": "^3.27.3",
		"@typescript-eslint/eslint-plugin": "^6.13.2",
		"@typescript-eslint/parser": "^6.13.2",
		"eslint": "8.55.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.0.1",
		"prettier": "^3.1.1",
		"prisma": "^5.7.0",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3",
		"vitest": "^1.0.4"
	},
	"imports": {
		"#config": "./dist/config.js",
		"#lib/*": "./dist/lib/*.js"
	}
}
