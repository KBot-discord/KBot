{
	"name": "@kbotdev/prettier-config",
	"version": "0.0.0",
	"license": "AGPL-3.0-or-later",
	"private": true,
	"main": "src/index.js",
	"exports": {
		".": {
			"import": "./src/index.js",
			"require": "./src/index.js"
		}
	},
	"scripts": {
		"lint": "eslint --ext js src",
		"lint:fix": "eslint --fix --ext js src",
		"format": "prettier --write --loglevel=warn \"src/**/*.js\""
	},
	"devDependencies": {
		"eslint": "^8.40.0",
		"prettier": "^2.8.8",
		"tsup": "^6.7.0"
	}
}
