{
	"name": "kbot",
	"version": "3.0.0",
	"description": "A multi-feature Discord bot",
	"author": "@kbotdev",
	"license": "AGPL-3.0-or-later",
	"type": "module",
	"main": "./dist/KBot.js",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/KBot-discord/kbot.git"
	},
	"homepage": "https://github.com/KBot-discord/kbot",
	"scripts": {
		"build": "pnpm check && tsup",
		"check": "tsc",
		"dev": "tsup --watch --onSuccess \"yarn start\"",
		"start": "node --enable-source-maps ./dist/KBot.js",
		"lint": "eslint --fix --ext ts src tests",
		"format": "prettier --write --log-level=warn \"{src,tests}/**/*.{js,ts,json}\"",
		"test": "vitest --watch=false",
		"test:watch": "vitest -w",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:migrate": "prisma migrate reset && prisma migrate dev",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs",
		"docker": "docker buildx build -t kbot/bot .",
		"dry-run": "pnpm build && pnpm lint && pnpm format"
	},
	"dependencies": {
		"@kbotdev/plugin-modules": "^3.0.0",
		"@killbasa/redis-utils": "^0.0.10",
		"@prisma/client": "^5.13.0",
		"@sapphire/decorators": "^6.1.0",
		"@sapphire/discord.js-utilities": "^7.2.0",
		"@sapphire/duration": "^1.1.2",
		"@sapphire/fetch": "^3.0.2",
		"@sapphire/framework": "^5.2.0",
		"@sapphire/plugin-api": "^6.1.1",
		"@sapphire/plugin-logger": "^4.0.2",
		"@sapphire/plugin-scheduled-tasks": "^10.0.1",
		"@sapphire/utilities": "^3.15.3",
		"@sentry/integrations": "^7.113.0",
		"@sentry/node": "^7.113.0",
		"@skyra/gifenc": "^1.0.1",
		"canvas": "2.11.2",
		"canvas-constructor": "^7.0.1",
		"colorette": "^2.0.20",
		"discord.js": "^14.14.1",
		"dotenv": "^16.4.5",
		"fuzzysort": "^2.0.4",
		"humanize-duration": "^3.32.0",
		"ioredis": "^5.4.1",
		"meilisearch": "^0.38.0",
		"prom-client": "^15.1.2"
	},
	"devDependencies": {
		"@killbasa/eslint-config": "^1.0.0",
		"@killbasa/prettier-config": "^1.0.0",
		"@killbasa/ts-config": "^1.0.0",
		"@sentry/types": "^7.113.0",
		"@swc/core": "^1.4.17",
		"@types/humanize-duration": "^3.27.4",
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"@typescript-eslint/parser": "^7.8.0",
		"eslint": "8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5",
		"prisma": "^5.13.0",
		"tslib": "^2.6.2",
		"tsup": "^8.0.2",
		"typescript": "^5.4.5",
		"vitepress": "^1.1.4",
		"vitest": "^1.5.3"
	},
	"lint-staged": {
		"*.{mjs,cjs,js,ts}": [
			"eslint --fix --ext js,ts",
			"prettier --write --log-level=warn \"**/*.{mjs,cjs,js,ts}\""
		],
		"*.{json,yml,yaml,md}": [
			"prettier --write --log-level=warn \"**/*.{json,yml,yaml,md}\""
		]
	},
	"engines": {
		"node": ">=20.x"
	},
	"volta": {
		"node": "22.0.0",
		"pnpm": "9.0.6"
	}
}
