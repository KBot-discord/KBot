{
	"name": "@kbotdev/bot",
	"version": "2.0.0",
	"description": "A multi-feature Discord bot",
	"author": "@kbotdev",
	"license": "AGPL-3.0-or-later",
	"main": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/KBot-discord/kbot.git",
		"directory": "apps/bot"
	},
	"homepage": "https://github.com/KBot-discord/kbot/tree/main/apps/bot",
	"engines": {
		"node": "^18.x"
	},
	"scripts": {
		"build": "yarn db:generate && tsc -b src",
		"dev": "tsc-watch --noClear -p src/tsconfig.json --onSuccess \"yarn run start\"",
		"start": "node ./dist/index.js",
		"start:migrate": "prisma migrate deploy && node ./dist/index.js",
		"lint": "eslint --fix --ext ts src tests prisma",
		"format": "prettier --write --loglevel=warn \"{src,tests,prisma}/**/*.{js,ts,json}\"",
		"test": "vitest",
		"db:generate": "ts-node ./prisma && prisma generate",
		"db:push": "prisma db push"
	},
	"dependencies": {
		"@bufbuild/connect-node": "^0.8.5",
		"@bufbuild/connect-web": "^0.8.5",
		"@bufbuild/protobuf": "^1.2.0",
		"@kbotdev/holodex": "workspace:^",
		"@kbotdev/menus": "^1.0.2",
		"@kbotdev/plugin-modules": "^1.0.6",
		"@kbotdev/proto": "*",
		"@prisma/client": "^4.12.0",
		"@sapphire/decorators": "^6.0.0",
		"@sapphire/discord.js-utilities": "^6.0.4",
		"@sapphire/duration": "^1.1.0",
		"@sapphire/fetch": "^2.4.1",
		"@sapphire/framework": "^4.2.2",
		"@sapphire/plugin-api": "^5.0.2",
		"@sapphire/plugin-i18next": "^5.0.2",
		"@sapphire/plugin-logger": "^3.0.2",
		"@sapphire/plugin-scheduled-tasks": "^6.0.0",
		"@sapphire/utilities": "^3.11.0",
		"@sentry/integrations": "^7.47.0",
		"@sentry/node": "^7.47.0",
		"@skyra/gifenc": "^1.0.1",
		"bullmq": "^3.10.4",
		"canvas": "2.10.2",
		"canvas-constructor": "^7.0.0",
		"discord-api-types": "^0.37.37",
		"discord.js": "^14.9.0",
		"dotenv": "^16.0.3",
		"humanize-duration": "^3.28.0",
		"ioredis": "^5.3.1",
		"meilisearch": "^0.32.3",
		"prom-client": "^14.2.0"
	},
	"devDependencies": {
		"@microsoft/tsdoc": "^0.14.2",
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/prettier-config": "^1.4.5",
		"@sentry/types": "^7.47.0",
		"@types/humanize-duration": "^3.27.1",
		"@types/node": "^18.15.11",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"@vitest/coverage-c8": "^0.30.0",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.1",
		"prettier": "^2.8.7",
		"prisma": "^4.12.0",
		"schemix": "^1.10.0",
		"ts-node": "^10.9.1",
		"tsc-watch": "^6.0.0",
		"typescript": "^4.9.5",
		"vitest": "^0.30.0"
	},
	"prettier": "@sapphire/prettier-config",
	"lint-staged": {
		"*.{mjs,ts,js}": [
			"eslint --fix",
			"prettier --write --loglevel=warn \"{src,tests,prisma}/**/*.{js,ts,json}\""
		]
	},
	"packageManager": "yarn@3.5.0",
	"imports": {
		"#prisma": "./prisma/generated/index.js",
		"#hooks/register": "./dist/lib/hooks/register.js",
		"#extensions/*": "./dist/lib/extensions/*.js",
		"#observability/*": "./dist/lib/observability/*.js",
		"#services/core": "./dist/lib/services/core/index.js",
		"#services/events": "./dist/lib/services/events/index.js",
		"#services/moderation": "./dist/lib/services/moderation/index.js",
		"#services/premium": "./dist/lib/services/premium/index.js",
		"#services/twitch": "./dist/lib/services/twitch/index.js",
		"#services/utility": "./dist/lib/services/utility/index.js",
		"#services/welcome": "./dist/lib/services/welcome/index.js",
		"#services/youtube": "./dist/lib/services/youtube/index.js",
		"#structures/*": "./dist/lib/structures/*.js",
		"#types/*": "./dist/lib/types/*.js",
		"#types/premium/*": "./dist/lib/types/premium/index.js",
		"#utils/*": "./dist/lib/utilities/*.js",
		"#utils/customIds": "./dist/lib/utilities/customIds/index.js",
		"#utils/validators": "./dist/lib/utilities/validators/index.js",
		"#modules/*": "./dist/modules/*.js",
		"#rpc/middlewares": "./dist/rpc/middlewares/index.js",
		"#rpc/server": "./dist/rpc/server.js"
	}
}
