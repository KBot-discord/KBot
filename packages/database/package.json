{
	"name": "@kbotdev/database",
	"version": "0.0.0",
	"license": "AGPL-3.0-or-later",
	"private": true,
	"main": "src/index.ts",
	"types": "src/index.ts",
	"scripts": {
		"lint": "eslint --fix --ext ts prisma",
		"format": "prettier --write --loglevel=warn \"prisma/**/*.{js,ts,json}\" && prisma format",
		"schemix": "ts-node ./prisma/schemix.ts",
		"db:generate": "yarn schemix && prisma generate",
		"db:push": "prisma db push",
		"db:deploy": "prisma migrate deploy"
	},
	"dependencies": {
		"@prisma/client": "^4.14.0"
	},
	"devDependencies": {
		"prettier": "^2.8.8",
		"prisma": "^4.14.0",
		"schemix": "^1.12.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	}
}
