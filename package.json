{
	"name": "@kbotdev/bot",
	"version": "2.0.0",
	"description": "A multi-feature Discord bot",
	"author": "@kbotdev",
	"license": "AGPL-3.0-or-later",
	"main": "./dist/index.js",
	"engines": {
		"node": "^18.x"
	},
	"scripts": {
		"prepare": "yarn buf:generate && yarn db:generate",
		"build": "tsc -b src",
		"dev": "tsc-watch --noClear -p src/tsconfig.json --onSuccess \"yarn run start\"",
		"start": "node ./dist/index.js",
		"start:migrate": "yarn db:deploy && node ./dist/index.js",
		"lint": "eslint --fix --ext ts src tests prisma",
		"format": "prettier --write --loglevel=warn \"{src,tests,prisma}/**/*.{js,ts,json}\"",
		"test": "vitest",
		"ci:build": "yarn db:generate && yarn build",
		"ci:test": "vitest run --coverage",
		"db:generate": "ts-node ./prisma && prisma generate",
		"db:dev": "prisma migrate dev",
		"db:push": "prisma db push",
		"db:deploy": "prisma migrate deploy",
		"buf:generate": "rm -r -f src/lib/rpc/gen && yarn buf:bot && yarn buf:twitter & yarn buf:youtube && yarn buf:types",
		"buf:bot": "buf generate buf.build/kbotdev/bot",
		"buf:twitter": "buf generate buf.build/kbotdev/twitter",
		"buf:youtube": "buf generate buf.build/kbotdev/youtube",
		"buf:types": "buf generate buf.build/kbotdev/types"
	},
	"dependencies": {
		"@bufbuild/connect-node": "^0.7.0",
		"@bufbuild/connect-web": "^0.7.0",
		"@bufbuild/protobuf": "^1.0.0",
		"@kbotdev/menus": "^1.0.2",
		"@kbotdev/plugin-modules": "^1.0.5",
		"@prisma/client": "^4.10.1",
		"@sapphire/decorators": "^6.0.0",
		"@sapphire/discord.js-utilities": "^6.0.2",
		"@sapphire/duration": "^1.0.0",
		"@sapphire/fetch": "^2.4.1",
		"@sapphire/framework": "^4.1.1",
		"@sapphire/plugin-api": "^5.0.1",
		"@sapphire/plugin-logger": "^3.0.1",
		"@sapphire/plugin-scheduled-tasks": "^6.0.0",
		"@sapphire/utilities": "^3.11.0",
		"@sentry/integrations": "^7.38.0",
		"@sentry/node": "^7.38.0",
		"@skyra/gifenc": "^1.0.1",
		"binarytf": "^2.1.2",
		"bullmq": "^3.7.1",
		"canvas": "2.10.2",
		"canvas-constructor": "^7.0.0",
		"discord-api-types": "^0.37.35",
		"discord.js": "^14.7.1",
		"dotenv": "^16.0.3",
		"ioredis": "^5.3.1",
		"prom-client": "^14.1.1"
	},
	"devDependencies": {
		"@bufbuild/protoc-gen-connect-web": "^0.7.0",
		"@bufbuild/protoc-gen-es": "^1.0.0",
		"@microsoft/tsdoc": "^0.14.2",
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^3.3.4",
		"@sentry/types": "^7.38.0",
		"@types/node": "^18.14.0",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"@vitest/coverage-c8": "^0.28.5",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-import-resolver-typescript": "^3.5.3",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.2",
		"prettier": "^2.8.4",
		"prisma": "^4.10.1",
		"schemix": "^1.8.5",
		"ts-node": "^10.9.1",
		"tsc-watch": "^6.0.0",
		"typescript": "^4.9.5",
		"vitest": "^0.28.5"
	},
	"resolutions": {
		"discord-api-types": "0.37.20"
	},
	"prettier": "@sapphire/prettier-config",
	"lint-staged": {
		"*.{mjs,ts,js}": [
			"eslint --fix",
			"prettier --write --loglevel=warn \"{src,tests,prisma}/**/*.{js,ts,json}\""
		]
	},
	"packageManager": "yarn@3.4.1",
	"imports": {
		"#prisma": "./prisma/generated/index.js",
		"#lib/*": "./dist/lib/*.js",
		"#lib/util": "./dist/lib/util/index.js",
		"#rpc/bot": "./dist/lib/rpc/bot.js",
		"#rpc/twitter": "./dist/lib/rpc/twitter.js",
		"#rpc/types": "./dist/lib/rpc/types.js",
		"#rpc/youtube": "./dist/lib/rpc/youtube.js",
		"#repositories/*": "./dist/lib/repositories/*.js",
		"#repositories": "./dist/lib/repositories/index.js",
		"#submodules/*": "./dist/lib/submodules/*.js",
		"#submodules": "./dist/lib/submodules/index.js",
		"#types/*": "./dist/lib/types/*.js",
		"#types/repositories": "./dist/lib/types/index.js",
		"#utils": "./dist/lib/util/index.js",
		"#utils/*": "./dist/lib/util/*.js",
		"#utils/customIds": "./dist/lib/util/customIds/index.js",
		"#utils/validators": "./dist/lib/util/validators/index.js",
		"#modules/*": "./dist/modules/*.js"
	}
}
