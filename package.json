{
	"name": "kbot",
	"version": "2.0.0",
	"description": "A multi-feature Discord bot",
	"author": "KBot",
	"license": "GPL-3.0",
	"main": "./dist/index.js",
	"scripts": {
		"build": "tsc -b src",
		"dev": "tsc-watch --noClear -p tsconfig.base.json --onSuccess \"npm run start\"",
		"start": "node ./dist/index.js",
		"test": "vitest",
		"test:ci": "vitest run --coverage",
		"prepare": "husky install",
		"db:generate": "ts-node ./prisma && prisma generate",
		"lint": "eslint --fix --ext .ts src tests prisma",
		"format": "prettier --write --loglevel=warn \"{src,tests,prisma}/**/*.{js,ts,json}\""
	},
	"dependencies": {
		"@discordjs/builders": "^1.3.0",
		"@prisma/client": "^4.5.0",
		"@sapphire/decorators": "^5.0.0",
		"@sapphire/discord.js-utilities": "^5.1.1",
		"@sapphire/duration": "^1.0.0",
		"@sapphire/framework": "^3.1.3",
		"@sapphire/pieces": "^3.6.0",
		"@sapphire/plugin-api": "^4.0.1",
		"@sapphire/plugin-logger": "^3.0.1",
		"@sapphire/plugin-scheduled-tasks": "^6.0.0",
		"@sapphire/plugin-subcommands": "^3.2.3",
		"@sapphire/utilities": "^3.11.0",
		"@sentry/integrations": "^7.17.4",
		"@sentry/node": "^7.17.4",
		"@skyra/gifenc": "^1.0.1",
		"axios": "^1.1.3",
		"binarytf": "^2.1.2",
		"bullmq": "^3.1.3",
		"canvas": "^2.10.2",
		"canvas-constructor": "^7.0.0",
		"discord-api-types": "^0.33.5",
		"discord.js": "^13.12.0",
		"express": "^4.18.2",
		"ioredis": "^5.2.3",
		"prisma": "^4.5.0",
		"prom-client": "^14.1.0",
		"schemix": "^1.8.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.2.0",
		"@commitlint/config-conventional": "^17.2.0",
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/prettier-config": "^1.4.4",
		"@sapphire/ts-config": "^3.3.4",
		"@sentry/types": "^7.17.4",
		"@types/express": "^4.17.14",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"@vitest/coverage-istanbul": "^0.24.5",
		"eslint": "^8.27.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"ts-node": "^10.9.1",
		"tsc-watch": "^5.0.3",
		"typescript": "^4.8.4",
		"vitest": "^0.24.5"
	},
	"prettier": "@sapphire/prettier-config",
	"lint-staged": {
		"*.{mjs,ts,js}": [
			"eslint --fix",
			"prettier --write --loglevel=warn \"{src,tests,prisma}/**/*.{js,ts,json}\""
		]
	}
}
