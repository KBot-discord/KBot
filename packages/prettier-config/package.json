{
	"name": "@kbotdev/prettier-config",
	"version": "0.0.0",
	"license": "AGPL-3.0-or-later",
	"private": true,
	"main": "dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "tsup",
		"check": "tsc",
		"lint": "eslint --ext ts src && yarn check",
		"lint:fix": "eslint --fix --ext ts src",
		"format": "prettier --write --loglevel=warn \"src/**/*.ts\""
	},
	"devDependencies": {
		"@kbotdev/eslint-config": "workspace:^",
		"eslint": "^8.40.0",
		"prettier": "^2.8.8",
		"tsup": "^6.7.0"
	}
}
